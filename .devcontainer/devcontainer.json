{
	"name": "CapriLLM",
	"image": "crpi-67joflzv7vnbp9lw.cn-hangzhou.personal.cr.aliyuncs.com/yykzjh1/caprillm_dev:latest",
	"mounts": [
		"source=/home/${localEnv:USER}/.ssh,target=/root/.ssh,type=bind",
